{
  "name": "redux-mithril-starter-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prebuild": "rimraf public/build",
    "build": "NODE_ENV=production webpack -p",
    "postbuild": "hashmark -l 16 -m assets.json 'public/build/*.{js,css}' 'public/build/{name}.{hash}{ext}'",
    "pretest": "eslint ./",
    "test": "mocha 'test/**/*.spec.js'",
    "test:watch": "nodemon -w ./ -e js -x 'npm test -- --growl --reporter dot'",
    "start": "node ./bin/www"
  },
  "engines": {
    "node": ">=4.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianmetcalf/redux-mithril-starter-app.git"
  },
  "author": "Ian Metcalf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ianmetcalf/redux-mithril-starter-app/issues"
  },
  "homepage": "https://github.com/ianmetcalf/redux-mithril-starter-app#readme",
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-mjsx": "^4.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "body-parser": "^1.15.0",
    "classnames": "^2.2.3",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "debug": "^2.2.0",
    "exports-loader": "^0.6.3",
    "express": "^4.13.4",
    "hashmark": "^3.1.0",
    "hbs": "^4.0.0",
    "imports-loader": "^0.6.5",
    "lodash": "^4.5.1",
    "mithril": "^0.2.2-rc.1",
    "morgan": "^1.7.0",
    "node-fetch": "^1.3.3",
    "normalizr": "^2.0.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.0",
    "redux-thunk": "^1.0.3",
    "reselect": "^2.0.3",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.14",
    "whatwg-fetch": "^0.11.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "babel-register": "^6.5.2",
    "deep-freeze": "0.0.1",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "expect": "^1.14.0",
    "fetch-mock": "^4.1.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.9.1",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
